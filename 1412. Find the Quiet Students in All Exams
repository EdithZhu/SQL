# Write your MySQL query statement below
SELECT S.STUDENT_ID, S.STUDENT_NAME FROM STUDENT S
WHERE S.STUDENT_ID IN (SELECT STUDENT_ID FROM EXAM)
AND S.STUDENT_ID NOT IN(
    SELECT E.STUDENT_ID FROM EXAM E
    WHERE (E.EXAM_ID,E.SCORE) IN (
        SELECT EXAM_ID, MAX(SCORE) FROM EXAM
        GROUP BY EXAM_ID
    )
    OR (E.EXAM_ID,E.SCORE) IN (
        SELECT EXAM_ID, MIN(SCORE) FROM EXAM
        GROUP BY EXAM_ID
    )
)
